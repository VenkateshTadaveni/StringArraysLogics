package com.logics;

import java.util.Arrays;

public class RemoveDuplicates {

	public static void main(String[] args) {
		int arr[] = {1,1,4,4,2,6,6};
		int rd=0;
		
		
		for(int i=1;i<arr.length;i++) {
			if(arr[rd]!=arr[i]) {
				rd++;
				arr[rd]=arr[i];
			}
			
		}
		System.out.println(Arrays.toString(arr));
		
		
		
}
}